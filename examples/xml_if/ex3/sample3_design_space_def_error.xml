<?xml version="1.0" encoding="UTF-8"?>
<!-- Example of XML front-end of the KisTA library -->
<!-- File for design space definition -->
<!--	FDL13 Example -->
<design_space xmlns="http://www.multicube.eu/" version="1.4">
	<simulator> 
		<simulator_executable path="/path/my_simulator_executable/kista-xml" />
	</simulator>
	<parameters>
		<parameter name="PE1_FREQ_VAL" description ="Frequency of first PE" type="integer" min="50" max="100"/>
	</parameters>
	<system_metrics> 
