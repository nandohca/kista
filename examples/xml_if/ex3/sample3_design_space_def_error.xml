	  <!-- name="rtos/sched_name/miss_deadlines"-->
	  <!-- name="rtos/sched_name/sched_utilization"-->
	  <!-- name="task/task_name/utilization"-->
	  <!-- name="task/task_name/miss_deadline" -->
	  <!-- Following two for periodic tasks -->
	  <!-- name="task/task_name/max_response_time" -->
	  <!-- name="task/task_name/avg_response_time" -->
	  <!-- name="task/task_name/std_dev_response_time" -->
	  <!-- name="task/task_name/location_of_worst_case_response_time" -->
	      <system_metric name="rtos/sched1/sched_utilization" type="float" unit="" />
  	      <system_metric name="rtos/sched2/plat_utilization" type="float" unit="M" />
		  <system_metric name="task/T1/max_response_time" type="float" unit="" />
