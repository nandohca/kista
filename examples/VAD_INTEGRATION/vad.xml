<?xml version="1.0" encoding="UTF-8"?>

<system>

	<environment name="EFRVocoder">
		<!-- Hook for environment global initialization -->
		<global_init name="env_init_fun" file="libenv_tasks_func.so"/>
		
		<!-- Hook for environment global wrap up -->
		<global_end name="env_end_fun" file="libenv_tasks_func.so"/>
		
		<!-- Environment actors -->
		<!-- Input actor -->
		<!-- A-DSE Task name = (rh, rl, scalACF, pitch, rc) -->
		<task name="T9">
			<!--
			<function name="VADsource_fun" file="libenv_tasks_func.so"/>
			-->
			<function name="VADFilesource_fun" file="libenv_tasks_func.so"/>
		</task>

		<!-- Output actor -->

		<!-- A-DSE Task name = VADflag -->
		<task name="T10">
			<!--
			<function name="VADsink_fun" file="libenv_tasks_func.so"/>
			-->
			<function name="VADFilesink_fun" file="libenv_tasks_func.so"/>
		</task>
	</environment>

	<application name="A1">
		<!-- Hook for system global initialization -->
		<global_init name="global_init_fun" file="tasks_func.so"/>
	
		<!-- Internal actors -->
		<!-- A-DSE Task name = ToneDetec -->
		<task name="T1">
			<function name="ToneDetection_fun" file="tasks_func.so"/>
			<task_properties>
				<WCET value="42180" unit="ns"/>
				<priority value="1"/>
				<!-- To be supported!! -->
				<WCEI value="2109" unit="instr" />
				<WCME value="250" unit="bytes"/>
			</task_properties>
		</task>

		<!-- A-DSE Task name = EnergComp -->
		<task name="T2">
			<function name="EnergyComputation_fun" file="tasks_func.so"/>
			<task_properties>
				<WCET value="43540" unit="ns"/>
				<priority value="1"/>
				<!-- To be supported!! -->
				<WCEI value="2177" unit="instr" />
				<WCME value="250" unit="bytes"/>
			</task_properties>
		</task>

		<!-- A-DSE Task name = ACFavg -->
		<task name="T3">
			<function name="ACFAveraging_fun" file="tasks_func.so"/>
			<task_properties>
				<WCET value="80120" unit="ns"/>
				<priority value="1"/>
				<!-- To be supported!! -->
				<WCEI value="4006" unit="instr" />
				<WCME value="250" unit="bytes"/>
			</task_properties>
		</task>

		<!-- A-DSE Task name = PredictVal -->
		<task name="T4">
			<function name="PredictorValues_fun" file="tasks_func.so"/>
			<task_properties>
				<WCET value="687700" unit="ns"/>
				<priority value="1"/>
				<!-- To be supported!! -->
				<WCEI value="34385" unit="instr" />
				<WCME value="250" unit="bytes"/>
			</task_properties>
		</task>

		<!-- A-DSE Task name = SpectrComp -->
		<task name="T5">
			<function name="SpectralComparison_fun" file="tasks_func.so"/>
			<task_properties>
				<WCET value="8624080" unit="ns"/>
				<priority value="1"/>
				<!-- To be supported!! -->
				<WCEI value="431204" unit="instr" />
				<WCME value="250" unit="bytes"/>
			</task_properties>
		</task>

		<!-- A-DSE Task name = ThreshAdapt -->
		<task name="T6">
			<function name="ThresholdAdaptation_fun" file="tasks_func.so"/>
			<task_properties>
				<WCET value="46840" unit="ns"/>
				<priority value="1"/>
				<!-- To be supported!! -->
				<WCEI value="2342" unit="instr" />
				<WCME value="250" unit="bytes"/>
			</task_properties>
		</task>

		<!-- A-DSE Task name = VADdec -->
		<task name="T7">
			<function name="VADdecision_fun" file="tasks_func.so"/>
			<task_properties>
				<WCET value="4020" unit="ns"/>
				<priority value="1"/>
				<!-- To be supported!! -->
				<WCEI value="201" unit="instr" />
				<WCME value="250" unit="bytes"/>
			</task_properties>
		</task>

		<!-- A-DSE Task name = VADhang -->
		<task name="T8">
			<function name="VADhangover_fun" file="tasks_func.so"/>
			<task_properties>
				<WCET value="4000" unit="ns"/>
				<priority value="1"/>
				<!-- To be supported!! -->
				<WCEI value="200" unit="instr" />
				<WCME value="250" unit="bytes"/>
			</task_properties>
		</task>

		<task name="D1_IN">
			<function name="d1in" file="tasks_func.so"/>
			<task_properties>
				<WCET value="1" unit="ns"/>
				<priority value="2"/>
				<period value="9" unit="ms"/>
				<!-- To be supported!! -->
			</task_properties>
		</task>
		
		<task name="D2_IN">
			<function name="d2in" file="tasks_func.so"/>
			<task_properties>
				<WCET value="1" unit="ns"/>
				<priority value="2"/>
				<period value="9" unit="ms"/>
				<!-- To be supported!! -->
			</task_properties>
		</task>
		
		<task name="D3_IN">
			<function name="d3in" file="tasks_func.so"/>
			<task_properties>
				<WCET value="1" unit="ns"/>
				<priority value="2"/>
				<period value="9" unit="ms"/>
				<!-- To be supported!! -->
			</task_properties>
		</task>
		
		<task name="D4_IN">
			<function name="d4in" file="tasks_func.so"/>
			<task_properties>
				<WCET value="1" unit="ns"/>
				<priority value="2"/>
				<period value="9" unit="ms"/>
				<!-- To be supported!! -->
			</task_properties>
		</task>
		
		<task name="D5_IN">
			<function name="d5in" file="tasks_func.so"/>
			<task_properties>
				<WCET value="1" unit="ns"/>
				<priority value="2"/>
				<period value="9" unit="ms"/>
			</task_properties>
		</task>
		<task name="D6_IN">
			<function name="d6in" file="tasks_func.so"/>
			<task_properties>
				<WCET value="1" unit="ns"/>
				<priority value="2"/>
				<period value="9" unit="ms"/>
			</task_properties>
		</task>
			
		<task name="D1_OUT">
			<function name="d1out" file="tasks_func.so"/>
			<task_properties>
				<WCET value="1" unit="ns"/>
				<priority value="2"/>
			</task_properties>
		</task>
		
		<task name="D2_OUT">
			<function name="d2out" file="tasks_func.so"/>
			<task_properties>
				<WCET value="1" unit="ns"/>
				<priority value="2"/>
				<!-- To be supported!! -->
			</task_properties>
		</task>
		
		<task name="D3_OUT">
			<function name="d3out" file="tasks_func.so"/>
			<task_properties>
				<WCET value="1" unit="ns"/>
				<priority value="2"/>
				<!-- To be supported!! -->
			</task_properties>
		</task>
		
		<task name="D4_OUT">
			<function name="d4out" file="tasks_func.so"/>
			<task_properties>
				<WCET value="1" unit="ns"/>
				<priority value="2"/>
			</task_properties>
		</task>
		
		<task name="D5_OUT">
			<function name="d5out" file="tasks_func.so"/>
			<task_properties>
				<WCET value="1" unit="ns"/>
				<priority value="2"/>
			</task_properties>
		</task>
		<task name="D6_OUT">
			<function name="d6out" file="tasks_func.so"/>
			<task_properties>
				<WCET value="1" unit="ns"/>
				<priority value="2"/>
			</task_properties>
		</task>
		<task name="BE1">
			<function name="be1" file="tasks_func.so"/>
			<task_properties>
				<WCET value="1" unit="ms"/>
				<priority value="3"/>
			</task_properties>
		</task>
		<task name="BE2">
			<function name="be2" file="tasks_func.so"/>
			<task_properties>
				<WCET value="1" unit="ms"/>
				<priority value="3"/>
			</task_properties>
		</task>
		
		<!-- Internal edges -->

		<!-- A-DSE Edge ID = 1 (L_av0) -->
		<!-- Src T3: ACFavg -->
		<!-- Dst T5: SpectrComp -->
		<comm_synch name="E1" src="T3" dest="T5" comm_type="fifo_buffer" comm_data_type="message" token_size="288"/>

		<!-- A-DSE Edge ID = 2 (L_av1) -->
		<!-- Src T3: ACFavg -->
		<!-- Dst T4: PredictVal -->
		<comm_synch name="E2" src="T3" dest="T4" comm_type="fifo_buffer" comm_data_type="message" token_size="288"/>

		<!-- A-DSE Edge ID = 3 (rav1_0 + scal_rav1_0) -->
		<!-- rav1_0 : short int (x9) -->
		<!-- scal_rav1_0 : short int (x1) -->
		<!-- Src T4: PredictVal -->
		<!-- Dst T5: SpectrComp -->
		<comm_synch name="E3" src="T4" dest="T5" comm_type="fifo_buffer" comm_data_type="message" token_size="160"/>

		<!-- A-DSE Edge ID = 4 (rav1_1 + scal_rav1_1) -->
		<!-- rav1_1 : short int (x9) -->
		<!-- scal_rav1_1 : short int (x1) -->
		<!-- Src T4: PredictVal -->
		<!-- Dst T6: ThreshAdapt -->
		<comm_synch name="E4" src="T4" dest="T6" comm_type="fifo_buffer" comm_data_type="message" token_size="160"/>

		<!-- A-DSE Edge ID = 5 (stat) -->
		<!-- Src T5: SpectrComp -->
		<!-- Dst T6: ThreshAdapt -->
		<comm_synch name="E5" src="T5" dest="T6" comm_type="fifo_buffer" comm_data_type="short"/>

		<!-- A-DSE Edge ID = 6 (acf0 + pvad_1) -->
		<!-- acf0 : struct {short int, short int} (x1) -->
		<!-- pvad_1 : struct {short int, short int} (x1) -->
		<!-- Src T2: EnergComp -->
		<!-- Dst T6: ThreshAdapt -->
		<comm_synch name="E6" src="T2" dest="T6" comm_type="fifo_buffer" comm_data_type="message" token_size="64"/>

		<!-- A-DSE Edge ID = 7 (rvad + scal_rvad) -->
		<!-- rvad : short int (x9) -->
		<!-- scal_rvad : short int (x1) -->
		<!-- Src T6: ThreshAdapt -->
		<!-- Dst T2: EnergComp -->
		<comm_synch name="E7" src="T6" dest="T2" comm_type="fifo_buffer" comm_data_type="message" token_size="160">
			<init_function name="init_rvad_fun" file="tasks_func.so"/>
		</comm_synch>

		<!-- A-DSE Edge ID = 8 (pvad_0) -->
		<!-- Src T2: EnergComp -->
		<!-- Dst T7: VADdec -->
		<comm_synch name="E8" src="T2" dest="T7" comm_type="fifo_buffer" comm_data_type="message" token_size="32"/>

		<!-- A-DSE Edge ID = 9 (tone) -->
		<!-- Src T1: ToneDetec -->
		<!-- Dst T6: ThreshAdapt -->
		<comm_synch name="E9" src="T1" dest="T6" comm_type="fifo_buffer" comm_data_type="short"/>

		<!-- A-DSE Edge ID = 10 (thvad) -->
		<!-- Src T6: ThreshAdapt -->
		<!-- Dst T7: VADdec -->
		<comm_synch name="E10" src="T6" dest="T7" comm_type="fifo_buffer" comm_data_type="message" token_size="32"/>

		<!-- A-DSE Edge ID = 11 (vvad) -->
		<!-- Src T7: VADdec -->
		<!-- Dst T8: VADhang -->
		<comm_synch name="E11" src="T7" dest="T8" comm_type="fifo_buffer" comm_data_type="short"/>


		<!-- Input edges -->

		<!-- A-DSE Edge ID = 12 (r_h_1 x9) -->
		<!-- Src T9: VADsource -->
		<!-- Dst T3: ACFavg -->		
		<comm_synch name="E12" src="T9" dest="T3" comm_type="fifo_buffer" comm_data_type="message" token_size="144" io="yes"/>

		<!-- A-DSE Edge ID = 13 (r_h_2 x9) -->
		<!-- Src T9: VADsource -->
		<!-- Dst T2: EnergComp -->
		<comm_synch name="E13" src="T9" dest="T2" comm_type="fifo_buffer" comm_data_type="message" token_size="144" io="yes"/>
		
		<!-- A-DSE Edge ID = 14 (r_l x9) -->
		<!-- Src T9: VADsource -->
		<!-- Dst T3: ACFavg -->
		<comm_synch name="E14" src="T9" dest="T3" comm_type="fifo_buffer" comm_data_type="message" token_size="144" io="yes"/>

		<!-- A-DSE Edge ID = 15 (scal_acf_1) -->
		<!-- Src T9: VADsource -->
		<!-- Dst T2: EnergComp -->
		<comm_synch name="E16" src="T9" dest="T2" comm_type="fifo_buffer" comm_data_type="short" io="yes"/>

		<!-- A-DSE Edge ID = 16 (scal_acf_2) -->
		<!-- Src T9: VADsource -->
		<!-- Dst T3: ACFavg -->
		<comm_synch name="E15" src="T9" dest="T3" comm_type="fifo_buffer" comm_data_type="short" io="yes"/>

		<!-- A-DSE Edge ID = 17 (rc_1 x4) -->
		<!-- Src T9: VADsource -->
		<!-- Dst T1: ToneDetec -->
		<comm_synch name="E17" src="T9" dest="T1" comm_type="fifo_buffer" comm_data_type="message" token_size="64" io="yes"/>

		<!-- A-DSE Edge ID = 18 (pitch) -->
		<!-- Src T9: VADsource -->
		<!-- Dst T6: ThreshAdapt -->
		<comm_synch name="E18" src="T9" dest="T6" comm_type="fifo_buffer" comm_data_type="short" io="yes"/>


		<!-- Output edges -->

		<!-- A-DSE Edge ID = 19 (vad) -->
		<!-- Src T8: VADhang -->
		<!-- Dst T10: VADflag -->
		<comm_synch name="E19" src="T8" dest="T10" comm_type="fifo_buffer" comm_data_type="short" io="yes"/>
		
		
		
		<comm_synch name="fifo1" src="_T0_M" dest="_T1_M" comm_type="fifo_buffer" comm_data_type="ushort"/>
		<comm_synch name="fifo2" src="_T2_M" dest="_T3_M" comm_type="fifo_buffer" comm_data_type="ushort"/>
		<comm_synch name="fifo3" src="_T4_M" dest="_T5_M" comm_type="fifo_buffer" comm_data_type="ushort"/>
		<comm_synch name="fifo4" src="_T6_M" dest="_T7_M" comm_type="fifo_buffer" comm_data_type="ushort"/>
		<comm_synch name="fifo5" src="_T8_M" dest="_T9_M" comm_type="fifo_buffer" comm_data_type="ushort"/>
		<comm_synch name="fifo6" src="_T10_M" dest="_T11_M" comm_type="fifo_buffer" comm_data_type="ushort"/>
		<comm_synch name="fifo7" src="_T12_M" dest="_T13_M" comm_type="fifo_buffer" comm_data_type="ushort"/>
		<comm_synch name="fifo8" src="_T14_M" dest="_T15_M" comm_type="fifo_buffer" comm_data_type="ushort"/>
		<comm_synch name="fifo9" src="_T16_M" dest="_T17_M" comm_type="fifo_buffer" comm_data_type="ushort"/>
		<comm_synch name="fifo10" src="_T18_M" dest="_T19_M" comm_type="fifo_buffer" comm_data_type="ushort"/>
		<comm_synch name="fifo11" src="_T20_M" dest="_T21_M" comm_type="fifo_buffer" comm_data_type="ushort"/>
		<comm_synch name="fifo12" src="_T22_M" dest="_T23_M" comm_type="fifo_buffer" comm_data_type="ushort"/>
		<comm_synch name="fifo13" src="_T24_M" dest="_T25_M" comm_type="fifo_buffer" comm_data_type="ushort"/>
		<comm_synch name="fifo14" src="_T26_M" dest="_T27_M" comm_type="fifo_buffer" comm_data_type="ushort"/>
	</application>


