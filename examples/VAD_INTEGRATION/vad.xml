		<HW_platform>

			<HW_Architecture name="arch1">

				<PE name="PE1" type="arm9">
<!--					
					<freq value="_PE1_FREQ" unit="MHz" />
-->
					<netif value="true"/>
				</PE>

				<PE name="PE2" type="arm9">
<!--
					<freq value="_PE2_FREQ" unit="MHz" />
-->
					<netif value="true"/>
				</PE>

				<PE name="PE3" type="arm9">
<!--
					<freq value="_PE3_FREQ" unit="MHz" />
-->
					<netif value="true"/>
				</PE>

				<PE name="PE4" type="arm9">
<!--
